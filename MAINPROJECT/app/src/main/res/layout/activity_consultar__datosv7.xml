<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.psanchez.mainproject.CONSULTAR_DATOS"
    android:id="@+id/CCONSULTA"
    android:background="@drawable/descarga">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CEDULA"
        android:id="@+id/textView6"
        android:layout_below="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/LABELCEDULA"
        android:layout_above="@+id/textView8"
        android:layout_alignLeft="@+id/LABELNOMBRE"
        android:layout_alignStart="@+id/LABELNOMBRE" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOMBRE"
        android:id="@+id/textView8"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/LABELNOMBRE"
        android:text="PABLO EJEMPLO"
        android:layout_alignTop="@+id/textView8"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FECHA"
        android:id="@+id/textView10"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/LABELFECHA"
        android:layout_alignTop="@+id/textView10"
        android:layout_alignLeft="@+id/LABELNOMBRE"
        android:layout_alignStart="@+id/LABELNOMBRE" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MESA"
        android:id="@+id/textView14"
        android:layout_marginTop="54dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/LABELMESA"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignLeft="@+id/LABELFECHA"
        android:layout_alignStart="@+id/LABELFECHA"
        android:text="12313" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/LABELSEXO"
        android:text="MASCULINO EJEMPLO"
        android:layout_above="@+id/textView10"
        android:layout_alignLeft="@+id/LABELNOMBRE"
        android:layout_alignStart="@+id/LABELNOMBRE" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NEXT"
        android:id="@+id/NEXTPAGEBUTON"
        android:enabled="false"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:layout_marginBottom="63dp"
        android:onClick="modificar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cedula"
        android:id="@+id/textView"
        android:textSize="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtcedulaconsulta"
        android:layout_alignParentTop="true"
        android:layout_marginTop="41dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:inputType="number" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cedula consulta"
        android:id="@+id/button3"
        android:layout_marginTop="50dp"
        android:textColor="#fff8ff55"
        android:onClick="consultarTalbal"
        android:layout_below="@+id/txtcedulaconsulta"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:nestedScrollingEnabled="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SEXO"
        android:id="@+id/textView7"
        android:layout_marginTop="34dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
